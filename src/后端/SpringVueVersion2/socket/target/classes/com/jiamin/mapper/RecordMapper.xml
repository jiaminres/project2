<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jiamin.mapper.RecordMapper">
    <insert id="insertOneRecord">
        insert into record values(#{groupID},#{sourceID},#{targetID},#{time,jdbcType=TIMESTAMP},#{message});
    </insert>
</mapper>