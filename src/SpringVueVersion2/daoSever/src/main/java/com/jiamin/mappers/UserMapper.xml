<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jiamin.mappers.UserMapper">
    <select id="selectAllUsersExceptOwn" resultType="org.example.common.entity.User">
        select * from User where accountID != #{accountID};
    </select>

    <select id="selectUserForLogin" resultType="org.example.common.entity.User">
        select * from User where accountID = #{accountID} and password = #{password}
    </select>
<!--    private String accountID;-->
<!--    private String userName;-->
<!--    private String HeadImageAddress;-->
    <insert id="insertOneUser">
        insert into User(accountID,password,userName,HeadImageAddress) values(#{accountID},#{password},#{userName},#{headImageAddress})
    </insert>
</mapper>